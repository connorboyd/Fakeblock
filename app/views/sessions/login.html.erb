<% @page_title = "Fakeblock | Login" -%>

<div class = "Sign_Form">
	<h1>Log in</h1>
	<%= form_tag(:action => 'login_attempt') do %>
		<p>Email: </br> <%= text_field_tag(:email) %></p>
		<p>Password:</br> <%= password_field_tag :login_password %></p>
		<%= submit_tag("Log In") %>
	<% end %>

	<a href="/create_account">Create a Fakeblock account</a>
</div>